{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en-GB,en",
    "enableGlobDot": false,
    "useGitignore": true,
    "dictionaries": [
        "poe-dict",
        "pob-dict",
        "ignore-dict",
        "extra-en-dict",
        "contribs-dict",
        "!aws",
        "!cryptocurrencies",
        "!ada",
        "lua",
        "cpp"
    ],
    "dictionaryDefinitions": [
        {
            "name": "poe-dict",
            "path": "./poe-dict.txt",
            "description": "Path of Exile dictionary",
            "addWords": true
        },
        {
            "name": "pob-dict",
            "path": "./pob-dict.txt",
            "description": "Path Of Building Community dictionary",
            "addWords": true
        },
        {
            "name": "ignore-dict",
            "path": "./ignore-dict.txt",
            "description": "Path Of Building Community ignore dictionary",
            "addWords": true,
            "noSuggest": true
        },
        {
            "name": "extra-en-dict",
            "path": "./extra-en-dict.txt",
            "description": "Extra English dictionary",
            "addWords": true
        },
        {
            "name": "contribs-dict",
            "path": "./contribs-dict.txt",
            "description": "Contrubutor's names dictionary",
            "addWords": true,
            "noSuggest": true
        }
    ],
    "ignoreRegExpList": [
        // Colour hex codes (eg ^xADAA47)
        "/\\^x[\\w]{6}/",
        // "manifeste?" ModParser.lua
        "/manifeste\\?/",
        // "]econds" Generated.lua
        "/\\]econds/",
        // "immun[" ModParser.lua
        "/immun\\[/",
        // "[hgdf]", "[viar]" ModParser.lua
        "/\\[(hgdf|viar)\\]/",
        // "[efcl]", "[erlg]", "[medh]" CalcPerform.lua
        "/\\[(efcl|erlg|medh)\\]/",
        // list of typos in generatedUniques.lua (may want to remove in future if PR #7245 isn't merged)
        "/\\[\"(Enchange|Blockedin|Areaof|Exposureon|Unaffectedby)\"\\]/",
        // "enem[yi]e?s?" src/Modules/ModParser.lua
        "/enem\\[yi\\]e\\?s\\?/",
    ],
    "globRoot": "${cwd}",
    "ignorePaths": [
        "*.dll",
        "*.exe",
        "*.png",
        "*.tga",
        "*.zip.part*",
        "*.zip",
        ".git",
        ".gitattributes",
        "**/cspell.json",
        "./runtime/*",
        "./src/Builds/*",
        "./src/Export/spec.lua",
        "./src/Settings.xml", // User file
        "Dockerfile",
        "docker-compose.yml",
        "buildDiff.sh",
        /* ************************ Auto-generated files from here down ************************ */
        "./src/Data/Bases/*",
        "./src/Data/ClusterJewels.lua",
        "./src/Data/Costs.lua",
        "./src/Data/Crucible.lua",
        "./src/Data/EnchantmentBelt.lua",
        "./src/Data/EnchantmentBody.lua",
        "./src/Data/EnchantmentBoots.lua",
        "./src/Data/EnchantmentFlask.lua",
        "./src/Data/EnchantmentGloves.lua",
        "./src/Data/EnchantmentHelmet.lua",
        "./src/Data/EnchantmentWeapon.lua",
        "./src/Data/Essence.lua",
        "./src/Data/Gems.lua",
        "./src/Data/LegionPassives.lua",
        "./src/Data/Minions.lua",
        "./src/Data/Misc.lua",
        "./src/Data/ModCache.lua",
        "./src/Data/ModCharm.lua",      // one of these two is wrong. fix when everything's settled
        "./src/Data/ModJewelCharm.lua", // 
        "./src/Data/ModFlask.lua",
        "./src/Data/ModItem.lua",
        "./src/Data/ModJewel.lua",
        "./src/Data/ModJewelAbyss.lua",
        "./src/Data/ModJewelCluster.lua",
        "./src/Data/ModMaster.lua",
        "./src/Data/ModVeiled.lua",
        "./src/Data/Pantheons.lua",
        "./src/Data/QueryMods.lua",
        "./src/Data/Skills/*.lua",
        "./src/Data/Spectres.lua",
        "./src/Data/StatDescriptions/*",
        "./src/Data/TattooPassives.lua",
        "./src/Data/TimelessJewelData/*",
        "./src/Data/Uniques/Special/WatchersEye.lua",
        "./src/Export/Minions/*",
        "./src/Export/Tree/*",
        "./src/TreeData/*",
        "manifest.xml",
    ],
    "flagWords": [
        "casted",
        "hte",
        "cant",
        "heres",
        "hows",
        "shes",
        "someones",
        "somethings",
        "thats",
        "theres",
        "whats",
        "whens",
        "wheres",
        "whys",
        "youd",
        "wont",
        "targetting",
        "therefor"
    ],
    "enableFiletypes": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "html",
        "ignore",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "lua",
        "markdown",
        "php",
        "plaintext",
        "properties",
        "python",
        "text",
        "typescript",
        "typescriptreact",
        "vue",
        "xml",
        "yaml",
        "yml"
    ]
}
