        "**/src/Export/Tree/*",
        "**/src/TreeData/*",
        "**/manifest.xml"
    ],
    "allowCompoundWords": false,
    "patterns": [],
    "ignoreRegExpList": [
        // Colour hex codes  (eg ^xADAA47)
        "/\\^x[\\w]{6}/",
        // "manifeste?", "leeche?"  src/Modules/ModParser.lua
        "/(manifeste|leeche)\\?/",
        // "]econds"  src/Data/Uniques/Special/Generated.lua
        "/\\]econds/",
        // "immun["  src/Modules/ModParser.lua
        "/immun\\[/",
        // "[hgdf]", "[viar]"  src/Modules/ModParser.lua
        "/\\[(hgdf|viar)\\]/",
        // "enem[yi]e?s?"  src/Modules/ModParser.lua
        "/enem\\[y/",
        // "satisi?fied"  src/Modules/ModParser.lua
        "/satisi\\?fied/",
        // ""Strenth""  src/Data/Uniques/Special/Generated.lua
        "/\"Strenth\"/",
        // "[cthd][ae][ukva]"  src/Modules/ModParser.lua
        "/\\[cthd\\]\\[ae\\]\\[ukva\\]/",
        // "<<HBGC04>>", "<<HBGAs>>"  (Harbinger Glyphs)  src/Data/FlavourText.lua
        "/<<HBG...?.?>>/",
        // "("Amluet","Amulet"):gsub("Botts","Boots")"  src/Export/Scripts/mapUniqueToFoulborn.lua
        "/Amluet\",\"Amulet/",
        "/Botts\",\"Boots/",
        // "Assets/itemsseparatorfoil.png"  [etc]  src/Classes/Tooltip.lua
        "/Assets\/[a-z]+.png/",
        // "[rR]ules"  src/Classes/TradeQueryRequests.lua
        "/\\[rR\\]ules/",
    ],
    "languageSettings": [],
    "reporters": [],
    "version": "0.2",
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "readonly": true,
    "dictionaryDefinitions": [
        {
            "name": "poe-dict",
            "path": "./poe-dict.txt",
            "description": "Path of Exile dictionary",
            "addWords": true
        },
        {
            "name": "pob-dict",
            "path": "./pob-dict.txt",
            "description": "Path Of Building Community dictionary",
            "addWords": true
        },
        {
            "name": "ignore-dict",
            "path": "./ignore-dict.txt",
            "description": "Path Of Building Community ignore dictionary",
            "addWords": true,
            "noSuggest": true
        },
        {
            "name": "extra-en-dict",
            "path": "./extra-en-dict.txt",
            "description": "Extra English dictionary",
            "addWords": true
        },
        {
            "name": "contribs-dict",
            "path": "./contribs-dict.txt",
            "description": "Contributor's names dictionary",
            "addWords": true,
            "noSuggest": true
        }
    ],
    "dictionaries": [
        "poe-dict",
        "pob-dict",
        "ignore-dict",
        "extra-en-dict",
        "contribs-dict",
        "companies",
